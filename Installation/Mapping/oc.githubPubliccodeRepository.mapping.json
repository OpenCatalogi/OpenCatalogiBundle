{
  "title": "GithubApiPubliccodeRepositoryToRepository",
  "$id": "https://api.github.com/oc.githubPubliccodeRepository.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.4",
  "passTrough": false,
  "mapping": {
    "name": "repository.name",
    "url": "repository.html_url",
    "publiccode_urls[]": "https://raw.githubusercontent.com/{{repository.full_name}}/{{urlReference}}/{{path}}",
    "source": "github"
  }
}
