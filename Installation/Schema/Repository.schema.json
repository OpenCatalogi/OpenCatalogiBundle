{
  "title": "Repository",
  "$id": "https://opencatalogi.nl/oc.repository.schema.json",
  "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
  "version": "0.1.6",
  "type": "object",
  "description": "",
  "properties": {
    "organisation": {
      "type": "object",
      "$ref": "https://opencatalogi.nl/oc.organisation.schema.json"
    },
    "components": {
      "type": "array",
      "items": {
        "$ref": "https://opencatalogi.nl/oc.component.schema.json"
      },
      "inversedBy": "url"
    },
    "source": {
      "type": "string",
      "enum": [
        "github",
        "gitlab"
      ]
    },
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "avatar_url": {
      "type": "string"
    },
    "last_change": {
      "type": "string"
    },
    "stars": {
      "type": "number"
    },
    "fork_count": {
      "type": "number"
    },
    "issue_open_count": {
      "type": "number"
    },
    "merge_request_open_count": {
      "type": "number"
    },
    "programming_languages": {
      "type": "array"
    },
    "topics": {
      "type": "array"
    },
    "content": {
      "type": "string"
    },
    "publiccode_urls": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "default_owner": {
      "type": "string"
    },
    "forked_from": {
      "type": "string"
    },
    "archived": {
      "type": "boolean"
    }
  }
}
