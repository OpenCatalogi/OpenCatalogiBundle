  "version": "0.1.0",
  "type": "object",
  "description": "",
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "shortDescription": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "detailPageImageUrl": {
      "type": "string"
    },
    "documentationUrl": {
      "type": "string"
    },
    "demoUrl": {
      "type": "string"
    },
    "bpmnProcessUrl": {
      "type": "string"
    },
    "isPublished": {
      "type": "boolean"
    },
    "owner": {
      "$ref": "https://opencatalogi.nl/oc.owner.schema.json"
    },
    "components": {
      "type": "array",
      "items": {
        "$ref": "https://opencatalogi.nl/oc.component.schema.json"
