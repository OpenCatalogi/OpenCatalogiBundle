{
  "title": "Repository",
  "$id": "https://opencatalogi.nl/oc.repository.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "version": "0.1.0",
  "type": "object",
  "description": "",
  "nameProperties": ["name"],
  "properties": {
    "organisation": {
      "$ref": "https://opencatalogi.nl/oc.organisation.schema.json"
    },
    "component": {
      "$ref": "https://opencatalogi.nl/oc.component.schema.json"
    },
    "source": {
      "type": "string",
      "enum": [
        "github",
        "gitlab"
      ]
    },
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "avatar_url": {
      "type": "string"
    },
    "last_change": {
      "type": "string"
    },
    "stars": {
      "type": "number"
    },
    "fork_count": {
      "type": "number"
    },
    "issue_open_count": {
      "type": "number"
    },
    "merge_request_open_count": {
      "type": "number"
    },
    "programming_languages": {
      "type": "array"
    },
    "topics": {
      "type": "array"
    },
    "content": {
      "type": "string"
    },
    "publiccode_url": {
      "type": "string"
    },
    "default_owner": {
      "type": "string"
    }
  }
}
