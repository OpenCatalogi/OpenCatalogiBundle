{
    "title": "GithubApiToRepository",
    "$id": "https://api.github.com/repositories",
    "$schema": "https://json-schema.org/draft/2020-12/mapping",
    "version": "0.0.2",
    "passTrough": false,
    "mapping": {
      "name": "name",
      "url": "html_url",
      "last_change": "updated_at",
      "stars": "stargazers_count",
      "fork_count": "forks_count",
      "issue_open_count": "open_issues_count",
      "source": "github"
    }
}
  
  