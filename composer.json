{
    "name": "open-catalogi/open-catalogi-bundle",
    "description": "An example package for creating symfony flex bundles as plugins",
    "type" : "symfony-bundle",	
"keywords" : [
	"commongateway",
	"common",
	"gateway",
	"conduction",
	"symfony",
	"common-gateway-plugin",
	"open catalogi"
],
"homepage" : "https://commongatway.nl",
    "license": "EUPL-1.2",
    "minimum-stability" : "dev",
    "require": {
        "php": ">=7.4",
		"commongateway/corebundle": "dev-version-oc"
	},
	"require-dev" : {
		"symfony/dependency-injection" : "~3.4|~4.1|~5.0"
	},
	"autoload" : {
		"psr-4" : {
			"OpenCatalogi\\OpenCatalogiBundle\\" : ""
		}
	},
	"scripts": {
		"post-package-install-cmd": "bin/console open-catalogi/open-catalogi-bundle",
		"post-package-update-cmd": "bin/console open-catalogi/open-catalogi-bundle",
		"post-package-uninstall-cmd": "bin/console open-catalogi/open-catalogi-bundle"
	}
}
